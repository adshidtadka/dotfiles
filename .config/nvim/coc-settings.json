{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "disableWorkspaceFolders": true,
      "filetypes": ["go"]
    }
  },
  "clangd.path": "~/dotfiles/.config/coc/extensions/coc-clangd-data/install/10.0.0/clangd_10.0.0/bin/clangd",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "yaml",
    "json",
    "typescript",
    "javascript",
    "html",
    "cpp",
    "vue"
  ]
}
