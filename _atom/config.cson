"*":
  Hydrogen: {}
  "atom-beautify":
    cpp:
      beautify_on_save: true
      default_beautifier: "clang-format"
    python:
      beautify_on_save: true
      sort_imports: true
  "atom-latex":
    build_after_save: false
    hide_log_if_success: true
    preview_after_build: "View in PDF viewer tab"
  "autocomplete-paths":
    scopes: []
  "autocomplete-plus":
    confirmCompletion: "tab always, enter when suggestion explicitly selected"
  "autocomplete-python":
    useKite: false
  core:
    disabledPackages: [
      "markdown-preview"
      "gpp-compiler"
      "markdown-scroll-sync"
      "emmet"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "*.aux"
      "*.bbl"
      "*.blg"
      "*.fdb_latexmk"
      "*.fff"
      "*.fls"
      "*.synctex.gz"
      "*.ttt"
    ]
    telemetryConsent: "no"
    themes: [
      "atom-material-ui"
      "atom-material-syntax"
    ]
  editor:
    fontFamily: "Ricty Diminished"
    fontSize: 15
    preferredLineLength: 100
    scrollPastEnd: true
    showIndentGuide: true
    softWrap: true
    tabLength: 4
  "exception-reporting":
    userId: "6f7b3b0e-bf39-46d9-95ce-d6fd2384407c"
  latex:
    buildOnSave: true
    engine: "platex"
    openResultInBackground: false
    opener: "pdf-view"
  "linter-gcc":
    gccLintOnTheFly: true
  "linter-ui-default":
    panelHeight: 222
    statusBarRepresents: "Current File"
  "markdown-preview-plus":
    mathConfig:
      enableLatexRenderingByDefault: true
    previewConfig:
      activatePreviewWithEditor: true
    syncConfig: {}
    useGitHubStyle: true
  minimap: {}
  "spell-check":
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "text.tex.latex"
    ]
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
".gfm.source":
  editor:
    tabLength: 4
